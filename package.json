{
  "name": "gtfs-to-chart",
  "version": "2.1.1",
  "private": false,
  "description": "Generate stringline charts of a transit routes from GTFS",
  "keywords": [
    "transit",
    "gtfs",
    "transportation",
    "visualizations",
    "stringline",
    "d3"
  ],
  "bugs": {
    "url": "https://github.com/blinktaginc/gtfs-to-chart/issues"
  },
  "repository": {
    "url": "https://github.com/BlinkTagInc/gtfs-to-chart.git"
  },
  "license": "MIT",
  "author": "Brendan Nee <brendan@blinktag.com>",
  "type": "module",
  "main": "index.js",
  "bin": {
    "gtfs-to-chart": "bin/gtfs-to-chart.js"
  },
  "scripts": {
    "start": "node ./app",
    "prepare": "husky"
  },
  "dependencies": {
    "better-copy": "^1.0.4",
    "chalk": "^5.6.2",
    "connect-slashes": "^1.4.0",
    "express": "^5.2.1",
    "gtfs": "^4.18.3",
    "js-beautify": "^1.15.4",
    "lodash-es": "^4.17.23",
    "moment": "^2.30.1",
    "progress": "^2.0.3",
    "pug": "^3.0.3",
    "sanitize-filename": "^1.6.3",
    "simplify-geojson": "^1.0.5",
    "sqlstring": "^2.3.3",
    "timer-machine": "^1.1.0",
    "untildify": "^6.0.0",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1"
  },
  "engines": {
    "node": ">= 22"
  },
  "release-it": {
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/keep-a-changelog": {
        "filename": "CHANGELOG.md"
      }
    }
  },
  "prettier": {
    "singleQuote": true
  },
  "lint-staged": {
    "*.json": "prettier --write",
    "*.{js,css,md}": "prettier --write"
  }
}
