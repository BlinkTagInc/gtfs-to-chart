{
  "name": "gtfs-to-chart",
  "version": "2.0.5",
  "private": false,
  "description": "Generate stringline charts of a transit routes from GTFS",
  "keywords": [
    "transit",
    "gtfs",
    "transportation",
    "visualizations",
    "stringline",
    "d3"
  ],
  "bugs": {
    "url": "https://github.com/blinktaginc/gtfs-to-chart/issues"
  },
  "repository": "git://github.com/blinktaginc/gtfs-to-chart",
  "license": "MIT",
  "author": "Brendan Nee <brendan@blinktag.com>",
  "type": "module",
  "main": "index.js",
  "bin": {
    "gtfs-to-chart": "bin/gtfs-to-chart.js"
  },
  "scripts": {
    "start": "node ./app",
    "lint": "eslint app/**/*.js lib/**/*.js public/**/*.js --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "better-copy": "^1.0.4",
    "chalk": "^5.2.0",
    "connect-slashes": "^1.4.0",
    "express": "^4.18.2",
    "gtfs": "^4.3.0",
    "js-beautify": "^1.14.7",
    "lodash-es": "^4.17.21",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "progress": "^2.0.3",
    "pug": "^3.0.2",
    "sanitize-filename": "^1.6.3",
    "simplify-geojson": "^1.0.5",
    "sqlstring": "^2.3.3",
    "timer-machine": "^1.1.0",
    "untildify": "^4.0.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-xo": "^0.43.1",
    "husky": "^8.0.3",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3"
  },
  "engines": {
    "node": ">= 14.x"
  },
  "release-it": {
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/keep-a-changelog": {
        "filename": "CHANGELOG.md"
      }
    }
  },
  "prettier": {
