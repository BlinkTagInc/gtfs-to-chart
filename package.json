{
  "name": "gtfs-to-chart",
  "version": "2.0.8",
  "private": false,
  "description": "Generate stringline charts of a transit routes from GTFS",
  "keywords": [
    "transit",
    "gtfs",
    "transportation",
    "visualizations",
    "stringline",
    "d3"
  ],
  "bugs": {
    "url": "https://github.com/blinktaginc/gtfs-to-chart/issues"
  },
  "repository": "git://github.com/blinktaginc/gtfs-to-chart",
  "license": "MIT",
  "author": "Brendan Nee <brendan@blinktag.com>",
  "type": "module",
  "main": "index.js",
  "bin": {
    "gtfs-to-chart": "bin/gtfs-to-chart.js"
  },
  "scripts": {
    "start": "node ./app",
    "prepare": "husky"
  },
  "dependencies": {
    "better-copy": "^1.0.4",
    "chalk": "^5.3.0",
    "connect-slashes": "^1.4.0",
    "express": "^4.21.1",
    "gtfs": "^4.15.3",
    "js-beautify": "^1.15.1",
    "lodash-es": "^4.17.21",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "progress": "^2.0.3",
    "pug": "^3.0.3",
    "sanitize-filename": "^1.6.3",
    "simplify-geojson": "^1.0.5",
    "sqlstring": "^2.3.3",
    "timer-machine": "^1.1.0",
    "untildify": "^5.0.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3"
  },
  "engines": {
    "node": ">= 20.11.0"
  },
  "release-it": {
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/keep-a-changelog": {
        "filename": "CHANGELOG.md"
      }
    }
  },
  "prettier": {
    "singleQuote": true
  },
  "lint-staged": {
    "*.json": "prettier --write",
    "*.{js,css,md}": "prettier --write"
  }
}
